{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "workorder",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1
      },
      "dueDate": {
        "type": "string"
      },
      "description": {
          "type": "string"
      },
      "done": {
          "type": "boolean"
      },
      "assignedTo": {
          "type": "string",
          "minLength": 1
      }
    },
    "required": [
      "name",
      "dueDate",
      "done",
      "assignedTo"
    ],
    "additionalProperties": false
  }
  